<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DreamTeamProj</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <res-ref-name>jdbc/my_db</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
   <listener>
    <listener-class>
              ua.nure.cache.java.service.MainListener
        </listener-class>
  </listener>
  <filter>
    <filter-name>PreProcessFilter</filter-name>
    <filter-class>ua.nure.cache.java.filters.PreProcessFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>PreProcessFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>ua.nure.cache.java.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/project/*</url-pattern>
  </servlet-mapping>
</web-app>